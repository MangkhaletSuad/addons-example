{
    "name": "Example add-on",
    "version": "Test",
    "slug": "example",
    "description": "Example add-on",
    "url": "https://github.com/MangkhaletSuad/addons-example.git",
    "startup": "services",
    "init": false,

    "ingress": true,
    "ingress_port": 8080,

    "parnel-icon": "mdi:cctv",
    "panel_title": "Example",
    "panel_admin": false,

    "port": {
        "8080/tcp": 8080
    },
    "host_network": false,
    "tmpfs": false,

    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "map": [
        "share:rw",
        "config:rw",
        "media:rw",
        "ssl"
    ],
    "image": "ghcr.io/home-assistant/{arch}-addon-example",
    "services": [
        "mqtt:want"
    ],
    "option": {
        "plugins": [],
        "custom_plugins": [],
        "env_vars": [],
        "ssl": true
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
        "plugins": [
            "str"
        ],
        "custom_plugins": {
            "name": "str",
            "url": "str",
            "unsigned": "bool?"
        },
        "ssl": "bool"
    }
}